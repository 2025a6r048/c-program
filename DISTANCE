include<stdio.h>
#include<math.h>
int main(){
 float x1,x2,y1,y2,distance;
 printf("ENTER VALUE OF X AND Y AXIS OF POITNT 1: ");   
 scanf("%f&f", &x1,&y1);   
 printf("ENTER VALUE OF X AND Y AXIS OF POITNT 2: ");      
 scanf("%f&f", &x2,&y2);   
distance=sqrt(pow(x2-x1,2)+ pow(y2-y1,2));   
 printf("distance btween two points is %f", distance);
 return 0;
}
